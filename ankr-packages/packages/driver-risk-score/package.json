{
  "name": "@ankr/driver-risk-score",
  "version": "1.0.0",
  "description": "Unified driver risk scoring combining telematics and wellness data for insurance underwriting",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "driver",
    "risk",
    "scoring",
    "telematics",
    "wellness",
    "insurance",
    "underwriting",
    "UBI",
    "usage-based-insurance"
  ],
  "author": "ANKR",
  "license": "MIT",
  "peerDependencies": {
    "@ankr/telematics-scoring": "^1.0.0",
    "@ankr/wellness-scoring": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@ankr/telematics-scoring": {
      "optional": true
    },
    "@ankr/wellness-scoring": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "registry": "http://localhost:4873"
  }
}
